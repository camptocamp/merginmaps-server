    "link:unregister": "echo '[@mergin/admin-app] Nothing to unregister.'",
    "lint": "eslint . --ext .js,.vue,.ts",
    "i18n:report": "vue-cli-service i18n:report --src './src/admin/**/*.?(js|vue)' --locales './src/admin/locales/**/*.json'"
  },
  "dependencies": {
